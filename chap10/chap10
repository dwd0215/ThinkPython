'''ä¸ä¼šï¼ï¼ï¼'''
def is_anagram(word1,word2):
    word1.sort()
    word2.sort()
    return word1 == word2

word1='spot'
word2='post'
print(is_anagram([word1,word2]))

#10.7
def has_duplicates(t):
    n=len(t)-1
    if n<=0:
        return False

    m=0
    t1=sorted(t)
    while m < n:
        for m in t1:
            if t1[m] == t1[m+1]:
                return True
            m=m+1
    return False

print(has_duplicates([1,2,5,3,4,2]))

#10.8
'''éœ€è¦å¸®å¿™debugä¸€ä¸‹ğŸ˜”'''
import random
def has_duplicates(t):
    n=len(t)-1
    if n<=0:
        return False

    m=0
    t1=sorted(t)
    while m < n:
        for m in t1:
            if t1[m] == t1[m+1]:
                return True
            m=m+1
    return False

def random_birthday(number):
    b=[]
    for i in number:
        add=random.randint(1,365)
        b.append(add)
    return b

def count(num_students,num_trials):
    count=0
    for i in num_trials:
        t=random_birthday(num_students)
        if has_duplicates(t):
            count +=1
    return count

num_students='23'
num_trials='1000'
print(count(num_students,num_trials))

#10.9
def way1():
    fin=open('words.txt')
    t=[]
    for line in fin:
        add=line.strip()
        t.append(add)
    return t

def way2():
    fin = open('words.txt')
    t = []
    for line in fin:
        add = line.strip()
        t=t+[add]
    return t

