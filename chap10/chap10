'''å†™æ³•ä¸€'''
def middle(t):
    return t[1:-1]

print(middle([1,2,3,4,5,6,7]))

'''å†™æ³•äºŒ'''
def middle(t):
    del t[0]
    del t[-1]
    return t

print(middle([1,2,3,4,5,6,7]))

