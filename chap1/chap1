#Exercese1.2
print(42*60+42) #%%1
print(10/1.61) #%%2
print((42*60+42)/(10/1.61)) #配速为412.48秒每英里
print((42/60+42)/(10/1.61)) #配速为6.87分每英里
print((10/1.61)/(42/3600+42/60))  #每小时跑了8.72英里
